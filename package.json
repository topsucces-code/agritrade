{
  "name": "agritrade-ai",
  "version": "1.0.0",
  "description": "Revolutionary AI-powered agricultural commodity trading platform connecting African farmers directly to buyers",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node backend/src/index.ts",
    "start": "node backend/dist/index.js",
    "build": "npm run build:backend",
    "build:backend": "tsc -p backend/tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint backend/src --ext .ts",
    "lint:fix": "eslint backend/src --ext .ts --fix",
    "format": "prettier --write backend/src/**/*.ts",
    "docker:build": "docker build -t agritrade-ai .",
    "docker:run": "docker run -p 3000:3000 agritrade-ai",
    "typecheck": "tsc --noEmit -p backend/tsconfig.json"
  },
  "keywords": [
    "agriculture",
    "trading",
    "AI",
    "machine-learning",
    "africa",
    "commodities",
    "blockchain",
    "fintech",
    "marketplace",
    "react-native",
    "nodejs",
    "mongodb"
  ],
  "author": {
    "name": "AgriTrade AI Team",
    "email": "dev@agritrade-ai.com",
    "url": "https://agritrade-ai.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/topsucces-code/agritrade#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/topsucces-code/agritrade.git"
  },
  "bugs": {
    "url": "https://github.com/topsucces-code/agritrade/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^7.2.4",
    "@fastify/multipart": "^7.6.0",
    "@fastify/rate-limit": "^8.0.3",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^1.10.0",
    "@google-cloud/vision": "^4.0.2",
    "@reduxjs/toolkit": "^2.8.2",
    "africastalking": "^0.6.0",
    "aws-sdk": "^2.1490.0",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.3",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "mongoose": "^7.6.3",
    "nodemailer": "^6.9.7",
    "redis": "^4.6.10",
    "sharp": "^0.32.6",
    "twilio": "^4.19.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/faker": "^6.6.9",
    "@types/jest": "^29.5.7",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.8.9",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "faker": "^6.6.6",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.1.1",
